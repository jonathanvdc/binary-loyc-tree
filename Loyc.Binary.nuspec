<?xml version="1.0"?>
<!-- NuGet package specification.
     Run `nuget pack -Version x.y.z Loyc.Binary.nuspec` to build Flame.x.y.z.nupkg -->
<package >
  <metadata>
    <id>Loyc.Binary</id>
    <version>$version$</version>
    <authors>Jonathan Van der Cruysse</authors>
    <owners>Jonathan Van der Cruysse</owners>
    <projectUrl>https://github.com/jonathanvdc/binary-loyc-tree</projectUrl>
    <!--<iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>A library that reads and writes Binary Loyc Tree (BLT) files.</summary>
    <description>Loyc.Binary is a library that can read and write Binary Loyc Tree (BLT) files: a binary encoding of LNode objects.</description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>Loyc binary format LNode BLT LES</tags>
    <dependencies>
        <dependency id="LoycCore" version="1.4.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="readme.txt" />
    <file src="Loyc.Binary\bin\Release\Loyc.Binary.dll" target="lib\net45\" />
    <file src="Loyc.Binary\bin\Release\Loyc.Binary.xml" target="lib\net45\" />
  </files>
</package>
